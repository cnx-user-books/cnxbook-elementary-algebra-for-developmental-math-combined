</exercise>
<exercise id="fs-id1170652652723">
<problem id="fs-id1170652652726">
<para id="fs-id1170652652728"><m:math><m:mrow><m:mn>12</m:mn><m:mo>÷</m:mo><m:mn>0.08</m:mn></m:mrow></m:math></para>
</problem>
<solution id="fs-id1170654144351">
<para id="fs-id1170654144353">150</para>
</solution>
</exercise>
<exercise id="fs-id1170654144358">
